    "#export\n",
    "def seq_cluster(items, cluster_label='', **kwargs):\n",
    "    sg = Cluster(cluster_label)\n",
    "    its = sg.add_items(*items, **kwargs)\n",
    "    sg.add_edges_seq(its)\n",
    "    return sg"
   ]
